{
  "name": "sosu",
  "version": "2.4.0",
  "description": "A music player for osu! songs built with Electron and React",
  "author": "Adivise",
  "main": "out/main/main.js",
  "scripts": {
    "sync-version": "node src/sync-version.js",
    "dev": "electron-vite dev",
    "start": "npm run build && electron-vite .",
    "clean": "npx rimraf out dist",
    "build": "npm run clean && npm run sync-version && npx electron-vite build",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "discord-rpc": "^4.0.1",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.294.0",
    "music-metadata": "^7.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^5.0.0",
    "terser": "^5.44.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
